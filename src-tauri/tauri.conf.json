{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri-app-demo",
  "version": "0.1.0",
  "identifier": "com.tauri-app-demo.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "tauri",
        "width": 800,
        "height": 700
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/48x48商城icon.png",
      "icons/96x96商城icon.png",
      "icons/96x96商城icon.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
